    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.11.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
